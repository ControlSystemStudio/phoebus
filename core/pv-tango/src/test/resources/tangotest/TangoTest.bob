  <widget type="textupdate" version="2.0.0">
    <name>Text Update_4</name>
    <pv_name>tango://sys/tg_test/1/State</pv_name>
    <x>170</x>
    <y>151</y>
    <width>120</width>
    <rules>
      <rule name="DevStateRule" prop_id="background_color" out_exp="false">
        <exp bool_exp="pv0 == 0">
          <value>
            <color name="ON/OPEN/EXTRACT" red="0" green="255" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 1">
          <value>
            <color name="OFF/CLOSE/INSERT" red="255" green="255" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 2">
          <value>
            <color name="OFF/CLOSE/INSERT" red="255" green="255" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 3">
          <value>
            <color name="ON/OPEN/EXTRACT" red="0" green="255" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 4">
          <value>
            <color name="OFF/CLOSE/INSERT" red="255" green="255" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 5">
          <value>
            <color name="ON/OPEN/EXTRACT" red="0" green="255" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 6">
          <value>
            <color name="MOVING/RUNNING" red="128" green="160" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 7">
          <value>
            <color name="STANDBY" red="255" green="255" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 8">
          <value>
            <color name="FAULT" red="255" green="0" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 9">
          <value>
            <color name="INIT" red="204" green="204" blue="122">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 10">
          <value>
            <color name="MOVING/RUNNING" red="128" green="160" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 11">
          <value>
            <color name="ALARM" red="255" green="140" blue="0">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 12">
          <value>
            <color name="DISABLE" red="255" green="0" blue="255">
            </color>
          </value>
        </exp>
        <exp bool_exp="pv0 == 13">
          <value>
            <color name="UNKNOWN" red="155" green="155" blue="155">
            </color>
          </value>
        </exp>
        <pv_name>$(pv_name)</pv_name>
      </rule>
    </rules>
  </widget>
